{
  "name": "dbc-can",
  "version": "0.1.0",
  "description": "A CAN DBC parsing library for javascript/typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "coverage": "jest --config jestconfig.json --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "npm" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags",
    "go": "tsc && node lib/index.js"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bit-dream/can-dbc.git"
  },
  "keywords": ["dbc","can","can-dbc","parser","vector"],
  "author": "Derek Black",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bit-dream/can-dbc/issues"
  },
  "homepage": "https://github.com/bit-dream/can-dbc#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  }
}
